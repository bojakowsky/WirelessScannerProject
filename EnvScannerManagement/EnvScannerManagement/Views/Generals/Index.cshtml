@model IEnumerable<Database.General>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Device.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AndroidAPI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfWifiConnections)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfBtConnections)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GPSlatitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GPSLongtitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateAndTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Device.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AndroidAPI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfWifiConnections)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfBtConnections)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GPSlatitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GPSLongtitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateAndTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
