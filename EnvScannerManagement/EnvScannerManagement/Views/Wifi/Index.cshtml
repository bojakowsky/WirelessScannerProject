@model IEnumerable<Database.Wifi>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Wifi data</h2>
<table class="table">
    <tr>
        <th>
            BSSID
        </th>
        <th>
            SSID
        </th>
        <th>
            Sec.
        </th>
        <th>
            Freq.
        </th>
        <th>
            Level
        </th>
        <th>
            Timestamp
        </th>
        <th>
            General ID
        </th>
        <th>
            General device ID
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>        
        <td>
            @Html.DisplayFor(modelItem => item.BSSID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SSID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Security)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Frequency)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Level)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GeneralId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.General.DeviceId)
        </td>
    </tr>
}

</table>
